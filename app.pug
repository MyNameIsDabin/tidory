doctype html

html(lang="ko")
	head
		//- TISTORY RSS
		link(rel="alternate", type="application/rss+xml", title="[##_title_##]", href="[##_rss_url_##]")
		//- Encoding
		meta(charset="utf-8")
		//- Only for Production
		unless htmlWebpackPlugin.options.dev
			link(rel="stylesheet" href='./style.css')

		//- End of Head
		block HEADER

	body(id="[##_body_id_##]")
		//- TISTORY main content
		block TISTORY

		//- webpack bundle
		script(src="./images/tidory.bundle.js")
		//- End of body
		block FOOTER